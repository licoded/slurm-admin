# Slurm Database Configuration
# Copy this file to .env and fill in your actual credentials

# Database Host
export SLM_DB_HOST="your_mysql_host"

# Database Port (default: 3306)
export SLM_DB_PORT="3306"

# Database User
export SLM_DB_USER="your_database_user"

# Database Password
export SLM_DB_PASSWORD="your_secure_password"

# Database Name
export SLM_DB_NAME="slurm_admin"

# Usage:
#   cp .env.example .env
#   # Edit .env with your credentials
#   source .env
#   ./slm run -- python script.py
